{
  "name": "bowlofsoup/cake-dto-mapper",
  "description": "Map a CakePHP entity into a DTO",
  "keywords": ["cakephp", "dto", "entity", "map", "mapper"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "BowlOfSoup",
      "homepage": "https://github.com/BowlOfSoup"
    }
  ],
  "require": {
    "php": ">=7.1",
    "cakephp/cakephp": ">=3.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "friendsofphp/php-cs-fixer": "^2.19"
  },
  "autoload": {
    "psr-4": { "BowlOfSoup\\CakeDtoMapper\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "BowlOfSoup\\CakeDtoMapper\\Tests\\": "tests/" }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-main": "0.1.0-dev"
    }
  }
}